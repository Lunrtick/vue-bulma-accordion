{
  "name": "vue-bulma-accordion",
  "description": "A simple, easily configurable accordion or collapsible for Vue, styled with Bulma",
  "version": "0.3.1",
  "author": {
    "name": "Gabriel Soicher",
    "email": "gabe@underware.co.za"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "Accordion",
    "Bulma",
    "Collapsible"
  ],
  "license": "MIT",
  "main": "dist/vue-bulma-accordion.common.js",
  "unpkg": "dist/vue-bulma-accordion.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lunrtick/vue-bulma-accordion.git"
  },
  "bugs": {
    "url": "https://github.com/Lunrtick/vue-bulma-accordion/issues"
  },
  "homepage": "https://lunrtick.github.io/vue-bulma-accordion/",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "vue-loader": "^10.0.0",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.15",
    "webpack": "^3.11.0",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "bulma": "^0.6.2",
    "vue": "^2.5.2"
  }
}
